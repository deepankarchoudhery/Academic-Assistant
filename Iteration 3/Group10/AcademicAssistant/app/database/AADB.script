CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE TERMS(TERMID INTEGER NOT NULL PRIMARY KEY,SEMESTER VARCHAR(20),YEAR INTEGER)
CREATE MEMORY TABLE COURSES(COURSEID INTEGER NOT NULL PRIMARY KEY,TERMID INTEGER,DEPARTMENT VARCHAR(20),NUMBER INTEGER,GRADE VARCHAR(4),TOTALMARKS DOUBLE,GOAL VARCHAR(4))
CREATE MEMORY TABLE COURSEELEMENTS(COURSEELEMENTID INTEGER NOT NULL PRIMARY KEY,COURSEID INTEGER,TITLE VARCHAR(45),DESCRIPTION VARCHAR(45),TYPE VARCHAR(20),TOTALMARKS DOUBLE,MARKSACHIEVED DOUBLE,WEIGHT DOUBLE)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
